<!-- Data ipa -->
<div class="container">
    <h3>anggota sains</h3>
    <!-- <a href="/add_science" class = "btn btn-primary">add user</a> -->
<table class="table">
  <thead> 
    <tr>
      
      <th scope="col">No</th>
      <th scope="col">nim</th>
      <th scope="col">nama</th>
      <th scope="col">kelas</th>
      <!-- <th scope="col">action</th> -->
    </tr>
  </thead>
  <tbody>
    <% 
    no=1
    ipa.forEach((science)=>{ %>
    <tr>
      <td><%= no++%></td>
      <td><%= science.nip %></td>
      <td><%= science.nama %></td>
      <td><%= science.id_kelas %></td>
      <!-- <td><a href="/edit_ipa/<%= science.id %>">EDIT</a> | <a href="/delete_ipa/<%= science.id %>">delete</a></td> -->
    </tr>
    <% }) %>
   
  </tbody>
</table>
</div>
<!-- ENd Data Ipa -->

